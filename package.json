    "docz": "^2.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.0",
    "flow-bin": "^0.89.0",
    "gatsby-theme-docz": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
