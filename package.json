    "clean-deep": "^3.2.0",
    "node-fetch": "^2.6.0",
    "promise-throttle": "^1.0.1",
    "qs": "^6.9.1",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
